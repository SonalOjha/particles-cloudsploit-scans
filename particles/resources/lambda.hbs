{{#resource logicalId='Lambda'}}
  "Type": "AWS::Lambda::Function",
  "Properties": {
    "Handler": "lambda.handler",
    "Role": { "Fn::GetAtt" : ["{{lambdaExecutionRoleLogicalId}}", "Arn"] },
    "Code": {
      "S3Bucket": "{{s3.bucket}}",
      "S3Key": "{{assetPath 'cloudsploit_scans.zip'}}"
    },
    "Runtime": "nodejs",
    "Timeout": "25"
  }
{{/resource}}
